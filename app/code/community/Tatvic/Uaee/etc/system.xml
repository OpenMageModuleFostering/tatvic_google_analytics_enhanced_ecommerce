<?xml version="1.0" encoding="UTF-8"?>
<config>
	<tabs>
        <tatvic translate="label" module="tatvic_uaee">
            <label>Tatvic Inc.</label>
            <sort_order>1</sort_order>
        </tatvic>
    </tabs>
    <sections>
		<tatvic_uaee module="tatvic_uaee">
			<label>Google Analytics Enhanced Ecommerce</label>
			<tab>tatvic</tab>
			<sort_order>100</sort_order>
			<show_in_default>1</show_in_default>
			<show_in_website>1</show_in_website>
			<show_in_store>1</show_in_store>
			<groups>
                <general translate="label" module="tatvic_uaee">
                    <label>General</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                     <comment><![CDATA[<a href="http://bit.ly/1JbU2lo" target="_blank"><img src="data:image/jpeg;base64," style="float:right" width=400 height=200/></a> <span style="float:right;clear:right;margin-right:13px"><strong><i>(The Ultimate Google Analytics Extension for Your Magento Store)</i></strong></span>
]]></comment>
                    <fields>
                        <enable>
                            <label>Activate Extension</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enable>
                        <account_id>
                            <label>GA Account ID</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<validate>required-entry</validate>
                            <comment>Enter your Google Analytics ID here. You can login into your Google Analytics account to find your ID e.g. UA-XXXXXXX-1</comment>
                        </account_id>
						<email_id>
                            <label>Email (Optional)</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<validate>validate-email</validate>
                            <comment>Provide your work email address to receive plugin enhancement update</comment>
                        </email_id>
                        
                    </fields>
                </general>
                <ecommerce translate="label" module="tatvic_uaee">
                    <label>Configuration</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>20</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                   
                    <fields>
                        <checkout_url>
                            <label>Checkout  URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>25</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Enter Your Checkout URL, Example - /checkout/onepage</comment>
                        </checkout_url>
                        <success_url>
                            <label>Checkout Success Page URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Enter Your Checkout Success Page URL, Example - /checkout/onepage/success</comment>
                        </success_url>
                         <gtmid>
                            <label>Use Our Extension with Your GTM </label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment><![CDATA[ If Yes, follow the instructions  <a href= "http://plugins.tatvic.com/downloads/magento-extension-GTM-steps-for-free-user.pdf" target="_blank">mention</a> in the document to learn how to set your GTM with our Extension.]]> </comment>
                         </gtmid>
                         <home_id>
                            <label>Home Page Category ID</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>45</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<validate>validate-number</validate>
                            <comment>Enter the category ID of the products that you want to showcase on the home page. You can find the category Id from Catalog > managed products section.</comment>
                        </home_id>
                         <brand>
                            <label>Brand Code (Optional)</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Provide the brand code to see the product brand name in GA.</comment>
                        </brand>
                        
                    </fields>
                </ecommerce>
						
			</groups>
		</tatvic_uaee>
    </sections>
</config>
